{
  "name": "Simplify Boox",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Make Boox web services simpler and easier to use",
  "homepage_url": "http://andrewshen.net",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "http://*.boox.com/",
    "https://*.boox.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.boox.com/",
        "https://*.boox.com/"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    }
  ]
}